import java.util.*;
import java.io.*;

public class Main{
    public static void escreverArquivo(int n, String arq){
        Scanner input = new Scanner(System.in);
        try(BufferedWriter escritor = new BufferedWriter(new FileWriter(arq))) {
		    for(int i = 0; i < n; i++){
		        String str = input.nextLine();
		        escritor.write(str);
		        escritor.newLine();
		    }
		} catch(Exception e){
		    e.printStackTrace();
		}
    }
    
    public static void lerArquivo(int n, String arq){
        Scanner input = new Scanner(System.in);
        try(BufferedReader leitor = new BufferedReader(new FileReader(arq))){
            for(int i = 0; i < n; i++){
                String linha = leitor.readLine();
                System.out.println(linha);
            }
        }catch(Exception e){
		    e.printStackTrace();
		}
    }
    
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		int n = input.nextInt();
		input.nextLine();
		String arq = "arquivo.txt";
		escreverArquivo(n, arq);
		lerArquivo(n, arq);
		input.close();
	}
}
