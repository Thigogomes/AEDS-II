public class Celula{
    public int elemento;
    public Celula prox;
    
    public Celula(){
        this.elemento = 0;
        this.prox = null;
    }
    
    public Celula(int elemento){
        this.elemento = elemento;
        this.prox = null;
    }
    
	public static void main(String[] args) {
		Celula temp = new Celula(1);
		temp.prox = new Celula(2);
		System.out.println(temp.elemento);
		System.out.println(temp.prox.elemento);
		System.out.println(temp.prox.prox);
	}
}
