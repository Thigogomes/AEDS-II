class No{
    public int elemento;
    public boolean cor;
    public No esq;
    public No dir;
    
    public No(){
        this.elemento = 0;
        this.cor = false;
        this.dir = null;
        this.esq = null;
    }
    
    public No(int elemento){
        this.elemento = elemento;
        this.cor = false;
        this.dir = null;
        this.esq = null;
    }
    
    public No(int elemento, boolean cor){
        this.elemento = elemento;
        this.cor = cor;
        this.dir = null;
        this.esq = null;
    }
}

public class ArvoreRB{
    public No raiz;
    
    public ArvoreRB(){
        this.raiz = null;
    }
    
    public ArvoreRB(No raiz){
        this.raiz = raiz;
    }
    
    public static void main(String[] args){
        ArvoreRB arvore = new ArvoreRB();
    }
}
